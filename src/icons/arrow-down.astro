---
interface Props {
  size?: number;
  class?: string;
}

const { size, class: className } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  width={size || "24"}
  height={size || "24"}
  viewBox="0 0 24 24"
  class={className}
  ><path fill="currentColor" d="M7.41 8.59L12 13.17l4.59-4.58L18 10l-6 6l-6-6z"
  ></path></svg
>
